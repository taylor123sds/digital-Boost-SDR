/**
 * ğŸ¯ GERA MENSAGEM PERSONALIZADA POR SETOR
 */
export function generateSectorMessage(leadName, sector, templateIndex = null) {
  const category = getSectorCategory(sector);
  const templates = SECTOR_PAIN_TEMPLATES[category] || SECTOR_PAIN_TEMPLATES.default;

  // Se templateIndex foi especificado, usar ele; senÃ£o, escolher aleatÃ³rio
  const index = templateIndex !== null ? templateIndex % templates.length : Math.floor(Math.random() * templates.length);
  const template = templates[index];

  // Substituir {NOME} pelo nome da empresa
  return template.replace(/{NOME}/g, leadName);
}
