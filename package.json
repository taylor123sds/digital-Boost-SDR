{
  "name": "agent-js-starter",
  "version": "1.0.0",
  "description": "Agente de IA completo em JavaScript (Node.js) com function calls",
  "type": "module",
  "scripts": {
    "start": "node start-orbion.js start",
    "start:3000": "node start-orbion.js start --port 3000",
    "start:3001": "node start-orbion.js start --port 3001",
    "start:force": "node start-orbion.js start --force",
    "start:web": "node src/server.js",
    "start:worker": "node src/worker.js",
    "start:all": "npm run start:web & npm run start:worker",
    "restart": "node start-orbion.js restart",
    "status": "node start-orbion.js status",
    "ports": "node start-orbion.js ports",
    "kill": "node start-orbion.js kill",
    "legacy": "node src/server.js",
    "worker": "node src/worker.js",
    "migrate": "node src/db/migrate.js",
    "migrate:status": "node -e \"import('./src/db/migrate.js').then(m => console.log(JSON.stringify(m.getMigrationStatus(), null, 2)))\"",
    "backup": "node scripts/backup_database.js backup",
    "backup:list": "node scripts/backup_database.js list",
    "backup:restore": "node scripts/backup_database.js restore",
    "lint": "eslint \"src/**/*.js\" --ignore-pattern \"src/v2/**\"",
    "lint:fix": "eslint \"src/**/*.js\" --ignore-pattern \"src/v2/**\" --fix",
    "lint:v2": "eslint \"src/v2/**/*.js\"",
    "lint:tenant": "node scripts/tenant_guardrail.js",
    "validate:api": "node scripts/validate-api-calls.js",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "test": "node test/test_v2_modules.js",
    "test:sheets": "node test/test_sheets.js",
    "test:sync": "node test/force_sync_leads.js"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@elevenlabs/elevenlabs-js": "^2.17.0",
    "@ffmpeg-installer/darwin-arm64": "^4.1.5",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.4.1",
    "body-parser": "^2.2.0",
    "chalk": "^5.6.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "fluent-ffmpeg": "^2.1.3",
    "googleapis": "^156.0.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "node-ffmpeg-stream": "^1.1.0",
    "node-wav": "^0.0.2",
    "nodemailer": "^7.0.11",
    "openai": "^4.52.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "puppeteer": "^24.26.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.34.3",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "winston": "^3.18.3",
    "xlsx": "^0.18.5",
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "eslint": "^9.39.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2"
  }
}
